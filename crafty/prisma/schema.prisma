generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserApiLimit {
  id        String   @id @default(cuid())
  userId    String   @unique// Make sure the casing here matches the column you're querying
  count     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
